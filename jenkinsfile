pipeline{
    stages{
        stage('mvn-build-project'){
            agent{
                docker{
                    image 'maven:3-alpine'
                    args '-v /root/.m2:/root/.m2'
                }
            }
            steps{
                sh 'mvn -B -gs maven-setting.xml -DskipTests clean package'
                sh 'echo 666'
                sh ```
                pwd
                ls -al
                ```
            }
        }
        stage(docker-build){
            sh 'echo 666'
        }
        stage(release-project){
            sh 'echo 666'
        }
    }
}
